{
  "name": "vite-react",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "env-cmd -f ./.env.local vite",
    "start:test": "env-cmd -f ./.env.development vite",
    "start:prod": "env-cmd -f ./.env.production vite",
    "build": "env-cmd -f ./.env.production tsc && vite build",
    "preview": "vite preview",
    "deploy-production": "env-cmd -f ./.env.production vite build && scp -r ./dist/* root@'YOUR IP':/var/www/'YOUR DOMAIN'/html"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "env-cmd": "^10.1.0",
    "node-sass": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.3.0",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/redux-logger": "^3.0.9",
    "@vitejs/plugin-react": "^1.0.7",
    "typescript": "^4.5.4",
    "vite": "^2.9.0"
  }
}
